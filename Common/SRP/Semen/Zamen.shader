			float4 Frag( VertexOutput packedInput  ) : SV_Target
			{
				UNITY_SETUP_INSTANCE_ID( packedInput );
				FragInputs input;
				ZERO_INITIALIZE( FragInputs, input );
				input.tangentToWorld = k_identity3x3;
				input.positionSS = packedInput.positionCS;

				PositionInputs posInput = GetPositionInput( input.positionSS.xy, _ScreenSize.zw, input.positionSS.z, input.positionSS.w, input.positionRWS );

				float3 V = float3( 1.0, 1.0, 1.0 );

				SurfaceDescription surfaceDescription = (SurfaceDescription)0;
				float3 ase_worldNormal = packedInput.ase_texcoord2.xyz;
				float3 ase_worldPos = packedInput.ase_texcoord3.xyz;
				float3 ase_worldViewDir = ( _WorldSpaceCameraPos.xyz - ase_worldPos );
				ase_worldViewDir = SafeNormalize( ase_worldViewDir );
				float dotResult34 = dot( reflect( ( -_DirectionalLightDatas[0].forward * float3( -1,-1,-1 ) ) , ( ase_worldNormal + float3( 0,0,0 ) ) ) , ase_worldViewDir );
				float saferPower45 = abs( max( dotResult34 , 0.0 ) );
				float spec55 = ( pow( saferPower45 , _SpecularShinines ) * _SpecularStrength );
				float4 spec_color125 = ( step( _SpecularThreshold , spec55 ) * _SpecularColor );
				float4 screenPos = packedInput.ase_texcoord4;
				float4 ase_screenPosNorm = screenPos / screenPos.w;
				ase_screenPosNorm.z = ( UNITY_NEAR_CLIP_VALUE >= 0 ) ? ase_screenPosNorm.z : ase_screenPosNorm.z * 0.5 + 0.5;
				float eyeDepth92 = LinearEyeDepth(SampleCameraDepth( ase_screenPosNorm.xy ),_ZBufferParams);
				float temp_output_105_0 = saturate( ( ( eyeDepth92 - screenPos.w ) * _FillingIntensity ) );
				float2 temp_cast_1 = (temp_output_105_0).xx;
				float depth_mask107 = (( _ToggleSwitch0 )?( tex2D( _EdgeFadeRamp, temp_cast_1 ).r ):( temp_output_105_0 ));
				float4 appendResult117 = (float4(_RimColor.r , _RimColor.g , _RimColor.b , ( _RimColor.a * depth_mask107 )));
				float3 normalizedWorldNormal = normalize( ase_worldNormal );
				float dotResult4 = dot( ase_worldViewDir , normalizedWorldNormal );
				float clampResult89 = clamp( pow( ( ( 1.0 - abs( dotResult4 ) ) + _RimShift ) , _RimToon ) , 0.0 , 1.0 );
				float4 lerpResult30 = lerp( _BaseColor , appendResult117 , clampResult89);
				float4 rim_color128 = lerpResult30;
				
				surfaceDescription.Color = ( ( spec_color125 + rim_color128 ) + ( depth_mask107 * _FillingColor ) ).rgb;
				surfaceDescription.Emission = 0;
				surfaceDescription.Alpha = saturate( ( (rim_color128).w + depth_mask107 ) );
				surfaceDescription.AlphaClipThreshold =  _AlphaCutoff;

				SurfaceData surfaceData;
				BuiltinData builtinData;
				GetSurfaceAndBuiltinData( surfaceDescription,input, V, posInput, surfaceData, builtinData );

				BSDFData bsdfData = ConvertSurfaceDataToBSDFData( input.positionSS.xy, surfaceData );
				LightTransportData lightTransportData = GetLightTransportData( surfaceData, builtinData, bsdfData );

				float4 res = float4( 0.0, 0.0, 0.0, 1.0 );
				UnityMetaInput metaInput;
				metaInput.Albedo = lightTransportData.diffuseColor.rgb;
				metaInput.Emission = lightTransportData.emissiveColor;
			#ifdef EDITOR_VISUALIZATION
				metaInput.VizUV = packedInput.VizUV;
				metaInput.LightCoord = packedInput.LightCoord;
			#endif
				res = UnityMetaFragment(metaInput);

				return res;
			}

